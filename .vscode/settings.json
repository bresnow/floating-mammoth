{
  "cSpell.ignoreWords": [
    "esnext",
    "macos",
    "perp"
  ],
  "git.enableSmartCommit": true,
  "[git-commit]": {

    "editor.rulers": [
      72
    ],
    "workbench.editor.restoreViewState": false
  },
  "git.autofetch": true,
  "git.autoStash": true,
  "git.fetchOnPull": true,
  "git.postCommitCommand": "sync",
  "git.scanRepositories": [
    "src"
  ]
}